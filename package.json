{
  "name": "react-validate",
  "version": "1.1.0",
  "description": "A simple, adaptable validation wrapper component. Small footprint, easy to use and to the point.",
  "files": [
    "*.md",
    "docs",
    "es6",
    "lib",
    "umd"
  ],
  "main": "lib/index",
  "jsnext:main": "es6/index",
  "scripts": {
    "build": "npm run build-cjs && npm run build-es",
    "build-cjs": "rimraf lib && cross-env BABEL_ENV=cjs babel ./components -d lib --ignore '__tests__'",
    "build-es": "rimraf es6 && cross-env BABEL_ENV=es babel ./components -d es6 --ignore '__tests__'",
    "start": "nodemon example/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lostpebble/react-validate.git"
  },
  "keywords": [
    "validation",
    "react"
  ],
  "author": "Paul Myburgh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lostpebble/react-validate/issues"
  },
  "homepage": "https://github.com/lostpebble/react-validate#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.13.0",
    "classnames": "^2.2.5",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "express": "^4.14.0",
    "express-urlrewrite": "^1.2.0",
    "material-ui": "^0.15.4",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.2",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "validator": "^5.5.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "lodash": "^4.15.0",
    "react": "^15.3.1"
  }
}
